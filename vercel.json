{
  
    "installCommand": "npm install",
    "type":"module",
  
    "headers": [
        {
          "source": "/p",
          "headers": [
            
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }
      ],
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api" },
        { "source": "/p", "destination": "/postEx.html" }
]
}